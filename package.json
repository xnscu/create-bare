{
  "name": "create-bare",
  "version": "0.3.0",
  "description": "create repo",
  "type": "module",
  "bin": {
    "create-bare": "outfile.cjs"
  },
  "files": [
    "outfile.cjs",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiangnanscu/create-bare.git"
  },
  "bugs": {
    "url": "https://github.com/xiangnanscu/create-bare/issues"
  },
  "homepage": "https://github.com/xiangnanscu/create-bare",
  "scripts": {
    "prerc": "npm --no-git-tag-version version minor",
    "rc": "yarn push",
    "commit": "git add . && git commit -am ",
    "c": "yarn commit",
    "g": "yarn push",
    "push": "yarn commit",
    "_push": "git push --set-upstream origin master",
    "_pull": "git pull origin",
    "postpush": "uname -s | grep -q Darwin && yarn _push || while true; do timeout 10 yarn _push && break; done",
    "pull": "uname -s | grep -q Darwin && yarn _pull || while true; do timeout 10 yarn _pull && break; done",
    "lab": "yarn sync && npm run build && ./outfile.cjs",
    "format": "prettier --write .",
    "build": "zx ./scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node20": "^20.1.2",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.16",
    "@types/prompts": "^2.4.9",
    "@vue/create-eslint-config": "^0.3.2",
    "@vue/tsconfig": "^0.5.1",
    "ejs": "^3.1.9",
    "esbuild": "^0.18.20",
    "esbuild-plugin-license": "^1.2.2",
    "gradient-string": "^2.0.2",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.1",
    "minimist": "^1.2.8",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "prompts": "^2.4.2",
    "zx": "^7.2.3"
  }
}
